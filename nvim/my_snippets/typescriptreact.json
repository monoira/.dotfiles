{
  "fetch with try/catch": {
    "prefix": "fetch",
    "body": [
      "const $1 = async () => {",
      "  try {",
      "    const res = await fetch($2);",
      "    if (res.ok) {",
      "      const data = await res.json();",
      "      set$3(data);",
      "",
      "      setIsLoading(false);",
      "      setError(null);",
      "    } else if (!res.ok) {",
      "      throw Error($4)",
      "    }",
      "  } catch (err) {",
      "    setIsLoading(false);",
      "",
      "    if (err instanceof Error) { ",
      "       setError(err.message);",
      "    }",
      "}"
    ],
    "description": "fetch with try/Catch"
  },
  "SEO component for react + typescript + helmet": {
    "prefix": "helmet-seo",
    "body": [
      "import { Helmet } from \"react-helmet-async\";",
      "",
      "type SEO_component_type = {",
      "  title: string;",
      "  description: string;",
      "  websiteName?: string;",
      "  type?: string;",
      "};",
      "",
      "export default function SEO({",
      "  title,",
      "  description,",
      "  websiteName = \"websitename123\",",
      "  type = \"website\",",
      "}: SEO_component_type) {",
      "  return (",
      "    <Helmet>",
      "      {/* || Standard metadata tags */}",
      "      <title>{title}</title>",
      "      <meta name=\"description\" content={description} />",
      "      <meta name=\"author\" content={websiteName} />",
      "",
      "      <html lang=\"en\" />",
      "      <meta charSet=\"UTF-8\" />",
      "      <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\" />",
      "      <meta",
      "        name=\"theme-color\"",
      "        media=\"(prefers-color-scheme: light)\"",
      "        content=\"cyan\"",
      "      />",
      "      <meta",
      "        name=\"theme-color\"",
      "        media=\"(prefers-color-scheme: dark)\"",
      "        content=\"black\"",
      "      />",
      "      {/* End standard metadata tags */}",
      "",
      "      {/* || Facebook tags */}",
      "      <meta property=\"og:type\" content={type} />",
      "      <meta property=\"og:title\" content={title} />",
      "      <meta property=\"og:description\" content={description} />",
      "      {/* OG image should be 1200x630 px */}",
      "      <meta property=\"og:image\" content=\"https://www.picsum.photos/1200/630\" />",
      "      <meta property=\"og:site_name\" content={websiteName} />",
      "      <meta",
      "        property=\"og:url\"",
      "        content=\"https://www.websitename123.netlify.app/\"",
      "      />",
      "      {/* End Facebook tags */}",
      "",
      "      {/* || Twitter tags */}",
      "      <meta name=\"twitter:creator\" content={websiteName} />",
      "      <meta name=\"twitter:card\" content={type} />",
      "      <meta name=\"twitter:title\" content={title} />",
      "      <meta name=\"twitter:description\" content={description} />",
      "      {/* End Twitter tags */}",
      "",
      "      {/* || Favicon tags */}",
      "      {/* USE https://favicon.io/favicon-converter/ TO GENERATE FAVICONS */}",
      "      {/* apple-touch-icon.png */}",
      "      <link",
      "        rel=\"apple-touch-icon\"",
      "        sizes=\"180x180\"",
      "        href=\"https://www.picsum.photos/180/180\"",
      "      />",
      "      {/* favicon-32x32.png */}",
      "      <link",
      "        rel=\"icon\"",
      "        type=\"image/png\"",
      "        sizes=\"32x32\"",
      "        href=\"https://www.picsum.photos/32/32\"",
      "      />",
      "      {/* favicon-16x16.png */}",
      "      <link",
      "        rel=\"icon\"",
      "        type=\"image/png\"",
      "        href=\"https://www.picsum.photos/16/16\"",
      "      />",
      "      <link",
      "        rel=\"manifest\"",
      "        href=\"https://faskrebuli.netlify.app/dist/pictures/favicons/site.webmanifest\"",
      "      />",
      "      {/* End favicon tags */}",
      "    </Helmet>",
      "  );",
      "}",
      ""
    ],
    "description": "SEO component for react + typescript + helmet"
  }
}
