{
  "SEO component for react + typescript + helmet": {
    "prefix": "helmet-seo",
    "body": [
      "import { Helmet } from \"react-helmet-async\";",
      "",
      "type SEO_component_type = {",
      "  title: string;",
      "  description: string;",
      "  websiteName?: string;",
      "  type?: string;",
      "};",
      "",
      "export default function SEO({",
      "  title,",
      "  description,",
      "  websiteName = \"websitename123\",",
      "  type = \"website\",",
      "}: SEO_component_type) {",
      "  return (",
      "    <Helmet>",
      "      {/* || REQUIRED meta tags */}",
      "      <title>{title}</title>",
      "      <meta charSet=\"UTF-8\" />",
      "      <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\" />",
      "",
      "      {/* || Less important meta tags */}",
      "      <meta name=\"description\" content={description} />",
      "      <meta name=\"author\" content={websiteName} />",
      "      <html lang=\"en\" />",
      "      <meta",
      "        name=\"theme-color\"",
      "        media=\"(prefers-color-scheme: light)\"",
      "        content=\"rgb(237, 241, 255)\"",
      "      />",
      "      <meta",
      "        name=\"theme-color\"",
      "        media=\"(prefers-color-scheme: dark)\"",
      "        content=\"rgb(52, 49, 72)\"",
      "      />",
      "",
      "      {/* || Facebook Open Graph tags */}",
      "      <meta property=\"og:type\" content={type} />",
      "      <meta property=\"og:title\" content={title} />",
      "      <meta property=\"og:description\" content={description} />",
      "      {/* og:image should be 1200x630 px */}",
      "      <meta property=\"og:image\" content=\"https://www.picsum.photos/1200/630\" />",
      "      <meta property=\"og:site_name\" content={websiteName} />",
      "      <meta",
      "        property=\"og:url\"",
      "        content=\"https://www.websitename123.netlify.app/\"",
      "      />",
      "",
      "      {/* || Twitter tags */}",
      "      <meta name=\"twitter:creator\" content={websiteName} />",
      "      <meta name=\"twitter:card\" content={type} />",
      "      <meta name=\"twitter:title\" content={title} />",
      "      <meta name=\"twitter:description\" content={description} />",
      "",
      "      {/* || Favicon tags */}",
      "      {/* USE https://favicon.io/favicon-converter/ TO GENERATE FAVICONS */}",
      "      {/* apple-touch-icon.png */}",
      "      <link",
      "        rel=\"apple-touch-icon\"",
      "        sizes=\"180x180\"",
      "        href=\"https://www.picsum.photos/180/180\"",
      "      />",
      "      {/* favicon-32x32.png */}",
      "      <link",
      "        rel=\"icon\"",
      "        type=\"image/png\"",
      "        sizes=\"32x32\"",
      "        href=\"https://www.picsum.photos/32/32\"",
      "      />",
      "      {/* favicon-16x16.png */}",
      "      <link",
      "        rel=\"icon\"",
      "        type=\"image/png\"",
      "        href=\"https://www.picsum.photos/16/16\"",
      "      />",
      "      <link",
      "        rel=\"manifest\"",
      "        href=\"https://faskrebuli.netlify.app/dist/pictures/favicons/site.webmanifest\"",
      "      />",
      "    </Helmet>",
      "  );",
      "}",
      ""
    ],
    "description": "SEO component for react + typescript + react-helmet-async"
  }
}
