{
  "Website head tags": {
    "prefix": "utils_html_head",
    "body": [
      "    <meta charset=\"UTF-8\" />",
      "    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\" />",
      "    <!-- || title -->",
      "    <title>BOILERPLATE_SITE_TITLE</title>",
      "    <meta name=\"twitter:title\" content=\"BOILERPLATE_SITE_TITLE\" />",
      "    <meta property=\"og:title\" content=\"BOILERPLATE_SITE_TITLE\" />",
      "    <!-- || description - around 150 characters -->",
      "    <meta",
      "      name=\"description\"",
      "      content=\"BOILERPLATE_DESCRIPTION\"",
      "    />",
      "    <meta",
      "      name=\"twitter:description\"",
      "      content=\"BOILERPLATE_DESCRIPTION\"",
      "    />",
      "    <meta",
      "      property=\"og:description\"",
      "      content=\"BOILERPLATE_DESCRIPTION\"",
      "    />",
      "    <!-- || static Facebook Open Graph tags -->",
      "    <!-- || make sure og:url content doesn't start with www -->",
      "    <meta property=\"og:url\" content=\"https://www.BOILERPLATE_SITENAME.netlify.app/\" />",
      "    <meta property=\"og:site_name\" content=\"BOILERPLATE_SITENAME\" />",
      "    <!-- || og:image and twitter:image -->",
      "    <!-- og:image should be 1200x630 px -->",
      "    <!-- twitter:image can be any size. -->",
      "    <!-- || make sure og:image and twitter:image content doesn't start with www -->",
      "    <meta property=\"og:image\" content=\"https://picsum.photos/1200/630\" />",
      "    <meta",
      "      property=\"twitter:image\"",
      "      content=\"https://picsum.photos/1200/630\"",
      "    />",
      "    <meta name=\"twitter:card\" content=\"summary_large_image\" />",
      "",
      "    <!-- || icons & webmanifest -->",
      "    <link",
      "      rel=\"apple-touch-icon\"",
      "      sizes=\"180x180\"",
      "      href=\"https://picsum.photos/180/180\"",
      "    />",
      "    <link",
      "      rel=\"icon\"",
      "      type=\"image/png\"",
      "      sizes=\"32x32\"",
      "      href=\"https://picsum.photos/32/32\"",
      "    />",
      "    <link",
      "      rel=\"icon\"",
      "      type=\"image/png\"",
      "      sizes=\"16x16\"",
      "      href=\"https://picsum.photos/16/16\"",
      "    />",
      "    <link",
      "      rel=\"manifest\"",
      "      href=\"http://percento.netlify.app/site.webmanifest\"",
      "    />"
    ],
    "description": "Website head tags"
  }
}
